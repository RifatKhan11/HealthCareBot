@using System.Collections.Generic
@using System.Linq
@using Microsoft.AspNetCore.Http
@using Microsoft.AspNetCore.Http.Authentication
@model LoginViewModel
@inject SignInManager<ApplicationUser> SignInManager

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" type="image/x-icon" href="~/security/img/fav.png">
    <title>Log In</title>
    <!-- bootstrap min css -->
    <link rel="stylesheet preload" href="~/security/css/fontawesome-6.css" as="style">
    <link rel="stylesheet preload" href="~/security/css/unicons.css" as="style">
    <link rel="stylesheet preload" href="~/security/css/swiper.min.css" as="style">
    <link rel="stylesheet preload" href="~/security/css/magnific-popup.css" as="style">
    <link rel="stylesheet preload" href="~/security/css/metismenu.css" as="style">
    <link rel="stylesheet preload" href="~/security/css/bootstrap.min.css" as="style">
    <!-- custom css style -->
    <link rel="stylesheet preload" href="~/security/css/style.css" as="style">
</head>

<body class="inner-page">
    <!-- login area start -->
    <div class="log-in-area-start rts-section-gap" style="padding-top: 50px;">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <!-- single login area start -->
                    <div class="single-form-s-wrapper" style="max-width: 430px; background-color: lightgray; padding: 20px;">
                        @* <img src="~/images/tota.png" style="width: 110px; margin: 10px; background: #464646; padding: 10px;" /> *@
                        <img src="~/chatbox/evercare.png" style="width: 140px; margin: 10px; padding: 10px;" />  

                        <div class="head">
                            <span>Welcome Back</span>
                            <h5 class="title">Login to continue</h5>
                        </div>
                        <div class="body">
                            <form asp-action="Login" method="post">
                                <div class="input-wrapper">
                                    <input asp-for="Name" type="text" placeholder="Enter your username" required>
                                    <input asp-for="Password" type="password" placeholder="Password">
                                </div>
                                <div class="check-wrapper">
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
                                        <label class="form-check-label" for="flexCheckDefault">
                                            Accept Terms & Conditions
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <a href="reset.html">Forgot password?</a>
                                    </div>
                                    
                                </div>
                                <button type="submit" class="rts-btn btn-primary">Log In</button>
                                <p>Don't have an account? <a class="ml--5" href="registration.html">Sign Up for Free</a></p>
                            </form>
                        </div>
