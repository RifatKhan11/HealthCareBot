
@{
    // Layout = "~/Views/Shared/_Layout.cshtml";
    Layout = null;

}
<!DOCTYPE html>
<html class="gr__cdn_chatbot_com">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>chatbot.com</title>
    <meta http-equiv="cache-control" content="max-age=0">
    <meta http-equiv="cache-control" content="no-cache">
    <meta http-equiv="expires" content="0">
    <meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT">
    <meta http-equiv="pragma" content="no-cache">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <link href="~/SweetAlert/sweetalert2.css" rel="stylesheet" />
    <link href="~/chatbox/Chat.css" rel="stylesheet">
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/bootstrap-theme.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/slick.css">
    <link rel="stylesheet" href="~/css/slick-theme.css">
    <link href="https://www.jqueryscript.net/css/jquerysctipttop.css" rel="stylesheet" type="text/css">

    <!-- SweetAlert CSS-->
    <link href="~/SweetAlert/sweetalert2.css" rel="stylesheet" />

    <style>
 
        .show {
            display: block;
        } 
        .btn-menu:hover {
            background-color: #3A6DFF;
            color: white !important;
        }


        .btn-group-message {
            background-color: white;
            color: #062A2F;
            font-weight: bold;
            padding: 4px 8px;
            border-top-right-radius: 10px;
            border-bottom-left-radius: 10px;
            border: 2px solid #062A2F;
            margin: 1px;
        }

            .btn-group-message:hover {
                color: white;
                background-color: #062A2F;
            } 
 

        .logo {
            width: 20%;
            margin-right: 10px;
            display: flex;
            justify-content: center;
            align-items: center;
            border: none;
        }

        .content {
            flex: 1;
        }

        .title {
            color: #000;
            font-weight: 500;
            font-size: 15px;
        }

        .subtitle {
            font-size: 11px;
            line-height: 1.3;
            letter-spacing: .2px;
            margin: 0;
            color: rgba(102,102,102,.8);
        }

        .hidden-wrapper {
            display: none;
            transition: display 5s ease;
        }








        /*Slick Carousel*/
        .container {
            display: flex;
            justify-content: center;
            align-items: center;
            padding: 0;
        }

        .autoplay {
            width: 610px;
            padding: 0;
            height: 160px;
            padding-left: 100px;
        }

        .slick-arrow {
            top: 107px;
        }

        .slick-slide {
            width: 200px !important;
            height: 200px !important;
            background-color: rosybrown;
            margin: 2px;
        }

            .slick-slide img {
                width: 100%;
            }




        .dropbtn {
            color: blueviolet;
        }

        .dr-box {
            display: flex;
            overflow-x: scroll;
            max-height: 280px;
        }

        ::-webkit-scrollbar {
            width: 10px;
        }

        /* Track */
        ::-webkit-scrollbar-track {
            box-shadow: inset 0 0 5px grey;
        }

        /* Handle */
        ::-webkit-scrollbar-thumb {
            background: lightgrey;
        }

            /* Handle on hover */
            ::-webkit-scrollbar-thumb:hover {
                background: lightgrey;
            }
 

        .btn-btm {
            background: rgb(0, 35, 85) !important;
            margin-top: 10px;
            display: inline-block;
            font-size: 16px;
            display: block;
            color: white !important;
            text-decoration: none !important;
            cursor: pointer;
        }

            .btn-btm:hover {
                background-color: lightgray !important;
                color: black !important;
                text-decoration: none !important;
            }

        
  




        .bubblemodal-content-body {
            position: relative;
            -webkit-box-flex: 1;
            -ms-flex: 1 1 auto;
            flex: 1 1 auto;
            padding: 1rem;
            font-size: 14px;
        }

        .bubblemodal-content-footer {
            display: -webkit-box;
            display: -ms-flexbox;
            display: flex;
            -webkit-box-align: center;
            -ms-flex-align: center;
            align-items: center;
            -webkit-box-pack: center;
            -ms-flex-pack: end;
            justify-content: center;
            padding: 1rem;
        }

        .btn-modal {
            display: inline-block;
            font-weight: 400;
            text-align: center;
            white-space: nowrap;
            vertical-align: middle;
            -webkit-user-select: none;
            -moz-user-select: none;
            -ms-user-select: none;
            user-select: none;
            border: 1px solid transparent;
            padding: 0.375rem 0.75rem;
            font-size: 11px;
            line-height: 1.5;
            border-radius: 0.25rem;
            transition: color .15s ease-in-out,background-color .15s ease-in-out,border-color .15s ease-in-out,box-shadow .15s ease-in-out;
            margin: 0 5px;
            background: rgba(0,0,0,.15);
            color: #fff;
        }

        .lazy-img-wrapper {
            border-radius: 50%;
        }

        .card-title {
            text-align: center;
            font-size: 15px;
            color: white;
            background-color: #002355;
            padding: 5px;
            margin: 0;
            cursor: pointer;
        }

        .card-box {
            border: 2px solid lightgray;
        }

            .card-box:hover {
                background-color: lightgray;
                cursor: pointer;
            }

        .btnbanglas:hover {
            background-color: #006CFF;
            color: white !important;
        }

        #gif img {
        }

        #myDropdown {
            background: #002355;
            color: white;
        }

        

        @@keyframes rotate {
            0% {
                transform: rotate(0deg);
            }

            100% {
                transform: rotate(360deg);
            }
        }

        /* Apply animation to the .fa-refresh element */
        .fa-refresh {
            animation: rotate 2s linear infinite;
        }

        .wrapper-list-item {
            overflow-y: scroll;
            height: 360px;
            padding-left: 10px;
            padding-bottom: 30px;
            border-top: 2px solid rgb(21, 210, 211);
            box-shadow: 0 0 5px 0.5px rgba(0,0,0,.15);
            margin: 0px 15px 10px;
            /* background: #0E2355;*/
        }

        .hide {
            display: none;
        }

        body {
            overflow-y: scroll !important;
            background: transparent;
        }

        ::-webkit-scrollbar {
            width: 4px;
            background: #7A6598 !important;
        }

        .wel-header {
            position: relative;
            opacity: 1;
            transform: translateY(0);
            margin: 0;
            padding: 10px;
        }

        .wel-header-user {
            height: 50px;
            width: 50px;
            height: 60px;
            width: 60px;
            background-color: #ffffff;
            border-radius: 4px;
            -webkit-box-shadow: 0 0 5px 2px rgba(0,0,0,.2);
            box-shadow: 0 0 5px 2px rgba(0,0,0,.2);
            background-size: contain;
            background-repeat: no-repeat;
            background-position: 50%;
            padding: 10px;
            margin: 0;
        }

        .hitheretext {
            width: calc(100% - 83px);
            padding-left: 10px;
            text-align: left;
            float: left;
        }

        .wel-subheading {
            padding-bottom: 10px;
            font-size: 12px;
            line-height: 16px;
            margin: 0;
        }

        .coronawrapper {
            position: relative;
            left: 0;
            right: 0;
            margin: 0;
            color: white;
            background: white;
            padding: 10px;
            color: black;
        }

        .card-svg-icon {
            width: 14px;
            height: 14px;
            vertical-align: middle;
            padding-right: 4px;
        }

     
       
        .closeChatBoxbtn {
            width: 35px;
            background: #0E2355;
            height: 35px;
            position: absolute;
            bottom: 0px;
            right: 0px;
            z-index: 100;
            border-radius: 50%;
        }
      
     
     
      
        #chatIcon {
            position: fixed;
            bottom: 122px;
            right: 30px;
            background: #41385c;
            width: 80px;
            height: 80px;
            border-radius: 50%;
            cursor: pointer;
            box-shadow: 0px 3px 16px 0px rgba(0, 0, 0, 0.6), 0 3px 1px -2px rgba(0, 0, 0, 0.2), 0 1px 5px 0 rgba(0, 0, 0, 0.12);
            animation: ripple 2s infinite;
        }

        .howyhelpicontext {
            font-size: 12px;
            width: 188px;
            cursor: pointer;
            z-index: 9999 !important;
            text-align: center;
            background: #41385d;
            color: #0eeee9;
            padding: 5px;
            display: inline-grid;
            bottom: 204px;
            position: fixed;
            right: 30px;
            border-radius: 100px;
            box-shadow: 0 2px 2px 1px rgba(0,0,0,.1);
        }

        .howyhelpicon {
            width: 88px;
            height: 108px; 
            cursor: pointer;
            z-index: 9999 !important;
            text-align: center;
            box-shadow: none;
            box-shadow: 0 2px 2px 1px rgba(0,0,0,.1);
        }


        @@keyframes ripple {
            0% {
                -moz-box-shadow: 0 0 0 0 rgba(0, 0, 0, 0.1);
                box-shadow: 0 0 0 0 rgba(204, 26, 26, 0.86);
            }

            100% {
                -moz-box-shadow: 0 0 0 30px rgba(0, 0, 0, 0);
                box-shadow: 0 0 0 30px rgba(0, 0, 0, 0);
            }
        }
    </style>
</head>
<body>

    <div id="chatIcon" class="btn-raised" style="z-index: 5;">
        <div id="chat-overlay"></div>


        <div class="howyhelp" id="cor">
            <div class="howyhelpicontext"> Hello, how can I help you?</div>
            <div class="howyhelpicon" style="background:  url(https://thb-website.s3.ap-south-1.amazonaws.com/plugins/chatbot/evercare/v1.0/chat-drop.png) 0 0 no-repeat;">
                <img src="https://healthylabs.s3-ap-southeast-1.amazonaws.com/chatbot/evercare.png" alt="" width="40px" class="mr-2" style="border: #fff solid 1px;
                                        border-radius: 100px;
                                        width: 66px;
                                        height: 66px;
                                        background: #fff;
                                        padding: 3px;
                                        margin-top: 10px;">
            </div>

        </div>


    </div>


     
    

     

    <script src="https://www.jqueryscript.net/demo/jQuery-Plugin-To-Create-Animated-Placeholders-placeholderTypewriter/placeholderTypewriter.js"></script>
 
    <script>
        localStorage.setItem('botKey', '@ViewBag.botKey');
        document.cookie = `${'botKey'}=${'@ViewBag.botKey'}`;
        botKeyNew = '@ViewBag.botKey';
        
        
       

    </script>
    <script src="~/chatbox/signalr/signalr.min.js"></script>
    <script crossorigin="anonymous" src="~/chatbox/Chat.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/popper.min.js"></script>
    <script src="~/chatbox/cookie.js"></script>
    <script src="~/js/slick.min.js"></script>


</body>
</html>
